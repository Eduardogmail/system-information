    <com.admob.android.ads.AdView  
        android:id="@+id/ad"
        android:layout_width="fill_parent" 
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        sysinfo:refreshInterval="60"
    />
    </RelativeLayout>-->
    
</merge>
